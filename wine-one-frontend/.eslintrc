{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "rules": {
    // "react/js-indent": [2, 2],
    "react/jsx-indent-props": [2, 2],
    "no-underscore-dangle":  ["error", { "allow": ["_id"] }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": "off"
  }
  // "parserOptions": {
  //   "ecmaVersion": "latest",
  //   "sourceType": "module"
  // }
}